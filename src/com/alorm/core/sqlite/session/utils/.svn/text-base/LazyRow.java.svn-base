package com.alorm.core.sqlite.session.utils;

public class LazyRow {

	private Class clstype;
	private String id;
	private Object obj;
	
	public LazyRow(Class clstype, String id,Object obj) {
		super();
		this.clstype = clstype;
		this.id = id;
		this.obj=obj;
	}
	public Class getClstype() {
		return clstype;
	}
	public String getId() {
		return id;
	}
	public Object getObj() {
		return obj;
	}
	
}
